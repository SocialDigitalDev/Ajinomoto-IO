{
	"header": {
		"blocks": [
			"header-layout.desktop",
			"header-layout.mobile"
		]
	},
	"header.full": {
		"blocks": [
			"header-layout.desktop",
			"header-layout.mobile"
		],
		"props": {
			"sticky": true,
			"fullWidth": false
		}
	},
	"header-layout.desktop": {
		"title": "HEADER DESKTOP",
		"children": [
			"helmet-component",
			"search-input-linx",
			"metodo-add-to-cart",
			"sticky-layout#header-desktop",
			"whatsapp-floating-button"
		],
		"props": {
			"blockClass": "header-desktop",
			"sticky": true,
			"fullWidth": false
		}
	},
	"header-layout.mobile": {
		"title": "HEADER MOBILE",
		"children": [
			"helmet-component",
			"search-input-linx",
			"metodo-add-to-cart",
			"sticky-layout#header-mobile",
			"whatsapp-floating-button"
		],
		"props": {
			"blockClass": "header-mobile",
			"sticky": true,
			"fullWidth": true
		}
	}
}