{
	"header": {
		"blocks": [
			"header-layout.desktop",
			"header-layout.mobile"
		]
	},

	"header.full": {
		"blocks": [
			"header-layout.desktop",
			"header-layout.mobile"
		],
		"props": {
			"sticky": true,
			"fullWidth": false
		}
	},

	"header-layout.desktop": {
		"title": "Header Desktop",
		"children": [
			"helmet-component",
			"sticky-layout#header-desktop"
		],
		"props": {
			"blockClass": "header-desktop",
			"sticky": true,
			"fullWidth": false
		}
	},

	"header-layout.mobile": {
		"title": "Header Mobile",
		"children": [
			"helmet-component",
			"sticky-layout#header-mobile"
		],
		"props": {
			"blockClass": "header-mobile",
			"sticky": true,
			"fullWidth": true
		}
	}
}
