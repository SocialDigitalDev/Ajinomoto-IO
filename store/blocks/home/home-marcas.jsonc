{
	"flex-layout.row#home-brands": {
		"title": "HOME MARCAS",
		"children": ["flex-layout.col#home-brands"],
		"props": {
			"blockClass": ["home-brands", "home-section"]
		}
	},

	"flex-layout.col#home-brands": {
		"title": "HOME brands",
		"children": ["rich-text#home-brands", "slider-layout#brands"],
		"props": {
			"blockClass": "home-brands",
			"preventHorizontalStretch": true,
			"preserveLayoutOnMobile": true
		}
	},

	"rich-text#home-brands": {
		"props": {
			"text": "## Nossas Marcas",
			"textPosition": "CENTER",
			"blockClass": "shelf-titles"
		}
	},

	"slider-layout#brands": {
		"props": {
			"itemsPerPage": {
				"desktop": 8,
				"tablet": 5.5,
				"phone": 2.5
			},
			"infinite": false,
			"showNavigationArrows": "desktopOnly",
			"showPaginationDots": "never",
			"blockClass": "home-brands",
			"navigationStep": "page",
			"arrowSize": 36
		},
		"children": [
			"info-card#brands-1",
			"info-card#brands-2",
			"info-card#brands-3",
			"info-card#brands-4",
			"info-card#brands-5",
