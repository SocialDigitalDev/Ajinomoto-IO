{
    "responsive-layout.phone#content-product": {
        "children": [
            "condition-layout.product#manioca-phone-validator"
        ]
        
    },
    "condition-layout.product#manioca-phone-validator":{
        "props":{
            "conditions": [
                {
                    "subject": "brandId",
                    "arguments": {
                      "id": "2097153890"
                    }
                }
            ],
          "matchType":"any",
          "Else": "responsive-layout.phone#content-product-inner",
          "Then": "responsive-layout.phone#content-product-manioca-inner"
        }
    },
    "responsive-layout.phone#content-product-inner":  {
        "children": [
            "flex-layout.row#img-product-info-mobile",
            "flex-layout.row#buy-together-desktop",
            "custom-pdp-recipies",
            "flex-layout.row#qvvt"
        ]
    },

    "responsive-layout.phone#content-product-manioca-inner":  {
        "children": [
            "flex-layout.row#img-product-info-mobile",
            "flex-layout.row#buy-together-desktop",
            "custom-pdp-recipies",
            "flex-layout.row#manioca-banner",
            "flex-layout.row#qvvt"
        ]
    }
}

